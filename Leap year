while True:

  try:

    year = int(input("Which year do you want to check? "))
    break

  except:

    print("Try again! ")
# ğŸš¨ Don't change the code above ğŸ‘†

#Write your code below this line ğŸ‘‡


if year % 4 == 0 :
  if year % 100 == 0:
    if year % 400 == 0:
      print("Leap Year. ")
    else:
      print("Not a Leap Year. ")
  else:
    print("Leap Year")


else:
  print("Not a Leap Year! ")
