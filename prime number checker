def prime_checker(n):
  result=[]
  for i in range(2,n-1):
    r = n % i
    result.append(r)
  if 0 in result:
      print('This is not a prime number ')
  else:
      print('This is a prime number ')


#Write your code above this line ğŸ‘†
    
#Do NOT change any of the code belowğŸ‘‡
n = int(input("Check this number: "))
prime_checker(n)



